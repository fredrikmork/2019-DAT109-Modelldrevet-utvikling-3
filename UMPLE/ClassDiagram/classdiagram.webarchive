    background: transparent;
    overflow: hidden;
}

#jjsPaper svg {
    background: transparent;
}

#jjsPaper svg .link {
    z-index: 2;
}

.html-element {
    position: absolute;
    /* Make sure events are propagated to the JointJS element so, e.g. dragging works.*/
    pointer-events: none;
    -webkit-user-select: none;
    border-radius: 1px;
    border: 1px solid grey;
    box-shadow: inset 0 0 1px black, 1px 1px 1px gray;
    padding: 5px;
