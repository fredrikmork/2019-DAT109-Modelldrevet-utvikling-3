.tabbtn {
  background-color: Transparent;
  border: none;
  margin: 0 0 0 10px;
  border-radius: 50%;
}
.tabbtn:hover {
  background-color: #CCC;
  color: white;
}
.tabname {
  padding-left: 1px;
}
.tabname br {
  display: none
}
