				}

				if (tempMed[0].charAt(0) === '-' || tempMed[0].charAt(0) === '#' || tempMed[0].charAt(0) === '~' || tempMed[0].charAt(0) === '+') {
					switch (tempMed[0].charAt(0)) {
						case "+":
