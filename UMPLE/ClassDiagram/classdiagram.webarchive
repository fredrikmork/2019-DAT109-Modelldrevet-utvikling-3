p.pagedescription { margin-left:5px; }

/* UNIVERSAL LAYOUT STYLES */ 
.inRow { float: left; }
.row { clear: both; overflow: auto; }

/* HEADER */
.logo { margin-top: 10px; }

/* MAIN APPLICATION CONTAINER */
#mainApplication { overflow: hidden; }

#paletteColumn { 
  box-sizing: border-box; -moz-box-sizing: border-box; border: 1px solid gray; 
  background-color: GhostWhite; padding: 1px 1px 1px 1px; -moz-border-radius: 4px; 
  -webkit-border-radius: 4px; border-radius: 4px; 
}
.ui-resizable-s { bottom: 0px; }

/* TEXT EDITORS */
textArea { 
  box-sizing:border-box; -moz-box-sizing:border-box; background-color: White; -moz-border-radius: 4px; 
  -webkit-border-radius: 4px; width:100%; height:100%; margin:0px; 
}
textArea.umpleModelEditor { resize:none; }
textArea.umpleLayoutEditor {overflow-y: scroll; resize: none; border: 1px solid gray; }
.CodeMirror-scroll { 
  box-sizing:border-box; -moz-box-sizing:border-box; -moz-border-radius: 4px; 
  -webkit-border-radius: 4px; border-radius: 4px;
}

/* Tweaks for the size and positioning of the resizing handle of the editor */
.ui-resizable-e { width: 15px; right: -10px; } 

/* CANVAS */
div.surface { 
  box-sizing:border-box; -moz-box-sizing:border-box; overflow:auto; display: block; width:100%; 
  height:100%; background-color: white; border: 1px solid gray; -moz-border-radius: 4px; 
  -webkit-border-radius: 4px; border-radius: 4px; position: relative;
}
div.surface:focus { outline:none; }
div.surface img { border:0; }
div.photoReady { border:0; overflow:visible; }
div.syncNeededMessage{ color: Gray; margin-left: 5px;}
.structureInCanvas { 
  width: 100%; height: 100%; border: 1px solid gray; -moz-border-radius: 4px; 
  -webkit-border-radius: 4px; border-radius: 4px; overflow: auto; 
  box-sizing: border-box; -moz-box-sizing: border-box;
}

.smallScreenCanvas { margin-top: 5px; margin-left: 0px; }
.smallScreenCanvasNoEditor { margin-top: 0px; margin-left: 5px; }
.smallScreenCanvasNoEditorNoPalette { margin-top: 0px; margin-left: 0px; }
.smallScreenEditor { margin-right: 5px; } 
.smallScreenEditorNoMargin { margin-right: 0px; } 

.largeScreenCanvas { margin-left: 5px; }
.largeScreenEditor { margin-right: 5px; }
.largeScreenCanvasNoMargin { margin-left: 0px; }
.largeScreenEditorNoMargin { margin-right: 0px; }

/* Tweaks for the size and positioning of the resizing handle of the canvas */
.ui-resizable-w { width: 15px; left:- -10px; } 

/* GENERATED CODE AREA */
#generatedCodeRow { 
  box-sizing:border-box; -moz-box-sizing:border-box; margin-top: 5px;
  -moz-border-radius: 4px; -webkit-border-radius: 4px; border-radius: 4px;
  }
#innerGeneratedCodeRow { margin-top: 5px; }
#messageArea { margin-left: 10px; }
.generatedDiagram { padding: 10px; overflow-x:auto; border: 1px solid gray; }
.generatedCode { overflow-x: hidden; border: 1px solid gray; }

/* UMPLE PALETTE */
.center-children { text-align: center; }
div.paletteWrapper { 
  box-sizing:border-box; -moz-box-sizing:border-box; border: 1px solid Gray; 
  background-color: GhostWhite; padding:  1px 1px 1px 1px; -moz-border-radius: 4px; 
  -webkit-border-radius: 4px; 
}
div.palette { height: 100%; width: 175px; font-family: Verdana; font-size: 9.5pt; }
div.palette img { cursor:pointer; margin-left: 8px; padding:2px; vertical-align:middle;}

div.palette ul { 
  background-color: GhostWhite; margin:5px -25px -15px -25px; padding:6px 0px 10px 0px; 
