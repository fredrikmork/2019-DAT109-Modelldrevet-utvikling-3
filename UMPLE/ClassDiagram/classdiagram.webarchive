					});

					//split modifier
					if (tempAttr[0].charAt(0) === '-' || tempAttr[0].charAt(0) === '#' || tempAttr[0].charAt(0) === '~' || tempAttr[0].charAt(0) === '+') {
						switch (tempAttr[0].charAt(0)) {
							case "+":
								attModifier = "public";
								break;
							case "~":
								attModifier = "package";
								break;
							case "#":
								attModifier = "protected";
								break;
							case "-":
								attModifier = "private";
								break;
							default:
								console.log('Invalid modifier in jjs_parser.js -->makeUmpleCodeFromClass');
						}
