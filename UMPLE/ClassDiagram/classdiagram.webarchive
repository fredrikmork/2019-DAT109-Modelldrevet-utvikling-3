					//Add other methods
					actionCodeObj.methods.push({
						"visibility": methodVisibility,
						"isAbstract": '',
						"type": tempMed[1],
						"name": tempMed[0],
						"parameters": tempParameters ? tempParameters[1] : [{}]
					});

				}
				var oldMethodType;
				var oldMethodVisibility;
