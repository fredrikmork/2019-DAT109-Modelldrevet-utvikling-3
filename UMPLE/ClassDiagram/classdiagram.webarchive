                updateFlag = true;
            }
        }
        
        this.resetBoxsize();

        setTimeout(_.bind(function () {
            if (updateFlag) {
                this.rerenderActivity();

                this.addListeners();
            }
        }, this), 200);
    },

    rerenderEvent: function(){
        //clear events
