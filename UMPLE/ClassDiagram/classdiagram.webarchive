    bottom: 0;
    left: 0;
    right: 0;
    color: #333;
    padding: 0px;
    height: 14px;
    font-family: arial, serif;
    font-size:12px;
    border: none;
    outline:none;
}

.html-element label {
    color: #333;
    text-shadow: 1px 0 0 lightgray;
    font-weight: bold;
}

.html-element span {
    position: absolute;
    top: 2px;
    right: 9px;
